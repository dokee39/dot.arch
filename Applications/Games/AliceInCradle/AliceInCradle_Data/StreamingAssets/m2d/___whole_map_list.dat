// %HEADER  <string> (<string>)  
// %REG  <string> (<string>)  
//      ... _2: ヘッダごとにダンジョンを指定する場合に入力 デフォルトは %DUNGEON の値が使われる

// WA のリンク に関してもここで行うよ


// %TEXT_KEY   <string>
//      ... Area_<string> の TX を使用する

// WA_LINK <src> <link1> <link2> ...
// link1, link2 のマップに侵入した場合でも src のマップを記録する

/* ___ __whole_forest ___ */
%DUNGEON    forest
%TEXT_KEY   forest
%HEADER     forest_
%BGM     BGM_forest Normal
%STORE_FLUSH_BATTLE '' FLUSH

%WA_LINK forest_mush_narrow forest_mush_coroseum
%WA_LINK forest_acid_lake forest_lava_coming
%WA_LINK forest_wood_hall forest_acid_dobadoba

/* ___ __whole_forest_hiroba ___ */
%DUNGEON    forest_hiroba
%TEXT_KEY   forest
%HEADER     foresth_
%BGM     BGM_forest Normal


/* ___ __whole_house ___ */
%DUNGEON    house
%TEXT_KEY   house
%HEADER     house_
%STORE_FLUSH Laevi 
// %DARK 1
%BGM     BGM_cornehl cornehl
%SAFE // 拠点である
%MATERIAL   snd.m2d_house


/* ___ __whole_city ___ */
%DUNGEON    city
%TEXT_KEY   city
%HEADER     city_
%REG    ^city_in_   city_in
%BGM     BGM_title grazia
%DARK
%SAFE
%EV_OMORASHI ___city/_omorashi
%MG_INITS Grazia
%MATERIAL   snd.m2d_house
%WA rect_already_known 1
%ZX_EVT ___city/_warp_zx

/* ___ __whole_school ___ */
%DUNGEON    city
%TEXT_KEY   school
%TEXT_KEY_AREATITLE city
%HEADER     school_
%REG    ^school_in_   city_in
%BGM     BGM_school BGM_school
%SAFE
%MG_INITS Grazia
%EV_OMORASHI ___city/_omorashi_scl
%MATERIAL   snd.m2d_house
%WA rect_already_known 1
%SHOW_ICON_ALWAYS 1
%ZX_EVT ___city/_warp_zx


/* ___ __whole_sacred ___ */
%DUNGEON    sacred
%TEXT_KEY   sacred
%HEADER     sacred_
%MG_INITS Sacred
%BGM     
%MATERIAL   snd.grazia_sacred